button 
    width 100%
    max-width 174px
    height 50px
    border-radius 5px
    box-sizing border-box
    border-style: none 
    cursor pointer
    font-size 18px
    text-align: center
    >img
        height 12.5px
.primaryNeutral
    background Blue
    color White
.primarySuccess
    background Green
    color White
.primaryDanger
    background Red
    color White    
.quartiarySuccess
    background none
    color Green
.quartiaryNeutral
    background transparent
    color Blue
    :disabled
        color lightBlue
